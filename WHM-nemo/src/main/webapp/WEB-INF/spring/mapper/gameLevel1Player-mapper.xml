<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.whm0304.nemo.persistance.GameLevel1PlayerDao">
	<select id="selectAll">
		<if test="p_id != null and p_id != ''">
		SELECT *
		FROM tbl_nemo
		WHERE p_id = #{p_id} AND n_num = '1'
		</if>
	</select>

</mapper>